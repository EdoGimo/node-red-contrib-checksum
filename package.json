{
	"name": "node-red-contrib-checksum",
	"version": "1.3.0",
	"description": "Calculates, using crypto-js, the checksum of a given string or file and compares it with the hash provided. Returns a boolean if the checksum is correct (true) or wrong (false).",
	"dependencies": {
		"crypto-js":"4.1.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/edogimo/node-red-contrib-checksum.git"
	  },
	"keywords": ["node-red", "checksum", "crypto-js", "sha512", "sha256", "sha1", "md5", "keccak", "ripemd160"],
	"node-red": {
		"nodes": {
			"Checksum":"checksum/checksum.js"
		}
	},
	"devDependencies": {
    		"mocha": "^9.1.3",
        	"node-red": "^2.1.2",
        	"node-red-node-test-helper": "^0.2.7",
        	"should": "^13.2.3"
    	},
    	"scripts": {
		"test": "mocha \"test/general_spec.js\""
    	}
}
